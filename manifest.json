{
  "manifest_version": 2,

  "name": "Leonardo Chrome Extension",
  "description": "This is a chrome extension for LeonardoJS",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["src/main.js"],
    "persistent": false
  },

  "permissions": [
    "activeTab"
  ]
}